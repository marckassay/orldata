{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resGroupName": {
      "value": "orldataRG"
    },
    "containerRegistryName": {
      "value": "orldataCR"
    },
    "keyVaultName": {
      "value": "orldataKV"
    },
    "userAssignedId": {
      "value": "orldataUId"
    },
    "hostName": {
      "value": "orldata"
    },
    "appServicePlanName": {
      "value": "orldataSP"
    },
    "resGroupLocation": {
      "value": "East US"
    },
    "dockerRegistryServerUsername": {
      "reference": {
        "keyVault": {
          "id": "<key-vault-resource-id>"
        },
        "secretName": "DOCKER-REGISTRY-SERVER-USERNAME"
      }
    },
    "dockerRegistryServerPassword": {
      "reference": {
        "keyVault": {
          "id": "<key-vault-resource-id>"
        },
        "secretName": "DOCKER-REGISTRY-SERVER-PASSWORD"
      }
    },
    "dockerRegistryServerUrl": {
      "reference": {
        "keyVault": {
          "id": "<key-vault-resource-id>"
        },
        "secretName": "DOCKER-REGISTRY-SERVER-URL"
      }
    }
  }
}
