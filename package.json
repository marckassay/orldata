{
  "name": "orldata",
  "version": "1.0.13",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "pre:build": "bash ./build/image/scripts/update-semver.sh && bash ./build/image/scripts/remove-outpath.sh && bash ./build/image/scripts/build-themes.sh",
    "pre:serve": "bash ./build/image/scripts/build-themes.sh",
    "build:production": "yarn run pre:build && ng build --configuration production",
    "serve:dev": "yarn run pre:serve && ng serve --configuration dev",
    "up:production": "bash ./build/image/scripts/update-semver.sh && docker-compose -f ./build/image/docker-compose.yml -f ./build/image/docker-compose.production.yml up -d --build",
    "up:dev": "bash ./build/image/scripts/update-semver.sh && bash ./build/image/scripts/set-image-tag.sh dev && docker-compose -f ./build/image/docker-compose.yml -f ./build/image/docker-compose.dev.yml up -d --build",
    "deployment": "pwsh -f ./build/deployment/scripts/deployment.ps1 -Verbose",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@azure/msal-angular": "1.0.0-beta.1",
    "@ngrx/effects": "~8.0.0",
    "@ngrx/entity": "~8.0.0",
    "@ngrx/router-store": "~8.0.0",
    "@ngrx/schematics": "~8.0.0",
    "@ngrx/store": "~8.0.0",
    "@ngrx/store-devtools": "~8.0.0",
    "angular-coordinates": "^1.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "msal": "1.2.1",
    "node-sass": "^4.12.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-import-rules": "^0.3.0",
    "typescript": "~3.4.5"
  }
}
