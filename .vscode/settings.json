{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.formatOnSave": true,
  "editor.rulers": [
    140
  ],
  "files.autoSave": "off",
  "markdownlint.config": {
    "MD014": false
  },
  "npm.packageManager": "yarn",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.settings.editor": "json",
  "files.eol": "\n"
}
